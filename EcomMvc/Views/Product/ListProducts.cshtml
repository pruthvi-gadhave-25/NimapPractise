

@model List<Product>

<h1>Products</h1>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var products in Model)
        {

            <tr>
                <td>@products.ProdcutName</td>

                <td>
                    <a asp-controller="Product" asp-action="Edit" asp-route-id="@products.ProductId">Edit</a>

                </td>
                <td>
                </td>
            </tr>

        }
    </tbody>

</table>
