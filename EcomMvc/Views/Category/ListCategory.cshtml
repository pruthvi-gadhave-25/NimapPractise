
@model List<Category>
<h1>Categories</h1>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
       
        </tr>
    </thead>
    <tbody>
        @foreach (var category in Model)
        {

            <tr>
                <td>@category.CategoryName</td>
            
                <td>
                    <a asp-controller="Student" asp-action="Edit" asp-route-id="@category.CategoryId">Edit</a>

                </td>
                <td>
                </td>
            </tr>

        }
    </tbody>

</table>